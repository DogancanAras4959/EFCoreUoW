@using UoW.DOMAIN.Models

@{
    List<Marka> markas = ViewBag.Markalar as List<Marka>;
}

<div class="col-lg-12">
    <div class="card">
        <div class="card-body">
            <div class="slick-table">
                @foreach (var item in markas)
                {
                    <div class="col-1">
                        <a href="@Url.Action("FilterByBrand","Home", new { MarkaID = item.ID })">
                            <img src="https://bayi.kiracelektrik.com.tr/Files/Bayi/@item.MarkaResim" class="img rounded-circle" height="70" style="padding:10px;" />
                        </a>
                    </div>
                }
            </div>

        </div>
    </div>
</div>